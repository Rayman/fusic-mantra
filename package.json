{
  "name": "mantra-demo",
  "version": "0.0.0",
  "options": {
    "mocha": "--require .scripts/mocha_runner imports/**/tests/**/*.js"
  },
  "scripts": {
    "lint": "eslint ./libs ./client ./server ./imports --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha imports/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx"
  },
  "devDependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "1.x.x",
    "eslint": "1.10.x",
    "eslint-plugin-react": "3.15.x",
    "mocha": "2.x.x",
    "react-addons-test-utils": ">=0.14.3 <=0.15.0",
    "sinon": "1.17.x"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "react": ">=0.14.3 <=0.15.0",
    "react-dom": ">=0.14.3 <=0.15.0",
    "react-komposer": "1.x.x",
    "react-simple-di": "1.x.x",
    "react-mounter": "1.x.x"
  },
  "private": true
}
